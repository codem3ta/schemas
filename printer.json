{
  "$id": "https://codemeta.de/schemas/printer",
  "type": "object",
  "collection": "assets",
  "allOf": [
    {
      "$ref": "https://codemeta.de/schemas/endpoint"
    }
  ],
  "i18n": {
    "de": {
      "singular": "Drucker",
      "plural": "Drucker",
      "description": "Drucker"
    }
  },
  "properties": {
    "type": {
      "type": "string",
      "const": "printer",
      "default": "printer"
    },
    "manufacturer": {
      "description": "The manufacturer of the printer.",
      "type": "string"
    },
    "model": {
      "description": "The Model of the printer.",
      "type": "string"
    },
    "name": {
      "description": "The Model of the printer.",
      "type": "string"
    },
    "assignedToPc": {
      "type": "array",
      "description": "An Array with the PCs connected to this Printer.",
      "items": {
        "type": "string",
        "relation": "manyToOne",
        "target": "https://codemeta.de/schemas/pc",
        "objectId": true,
        "database": "config"
      }
    },
    "room ": {
      "description": "The room where the printer is isntalled.",
      "type": "string",
      "relation": "manyToOne",
      "target": "https://codemeta.de/schemas/room",
      "objectId": true,
      "database": "config"
    },
    "interface": {
      "description": "The interface with the credentials.",
      "type": "string",
      "relation": "manyToOne",
      "target": "https://codemeta.de/schemas/interface",
      "objectId": true,
      "database": "config"
    }
  },
  "unevaluatedProperties": true,
  "required": ["title"],
  "name": "printer"
}
